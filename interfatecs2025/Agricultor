#include <stdio.h>
int main(){
int N, P;
float T, U;

    scanf("%d",&N);
    int contador[N];

    for(int i = 0; i < N; i++) {
        scanf("%f ", &T);
        scanf("%f ",&U);
        scanf("%d",&P);
        if(P == 1)
            contador[i] = 0;

        else {
            if(T > 30 && U < 50)
                contador[i] = 1;
            else
                contador[i] = 0;
        }
    }
    for(int i = 0; i < N; i++) {
        if(contador[i] == 0)
            printf("NAO REGAR\n");
        else
            printf("REGAR\n");
    }
}
